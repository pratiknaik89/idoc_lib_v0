{
  "name": "idoceditor",
  "version": "0.0.1",
  "scripts": {
    "publishlib":"npm run lib-dispatch",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf dist",
    "editor-clean": "rimraf dist/doceditor",
    "editor-b": "ng build doceditor",
    "editor-assets": "gulp build-assets --option doceditor",
    "editor-build": "npm run editor-clean && npm run editor-b && wait-on dist/doceditor/public-api.d.ts && npm run editor-assets",
    "editor-watch": "editor-b  --watch",
    "viewer-clean": "rimraf dist/docviewer",
    "viewer-b": "ng build docviewer",
    "viewer-assets": "gulp build-assets --option docviewer",
    "viewer-build": "npm run viewer-clean && npm run viewer-b && wait-on dist/docviewer/public-api.d.ts && npm run viewer-assets",
    "viewer-watch": "viewer-b --watch",
    "lib-build": "ng build esigndoccontrol",
    "lib-watch": "ng build esigndoccontrol --watch",
    "serve-watch": "wait-on dist/esigndoccontrol/public-api.d.ts && ng serve --port 1212",
    "build-assets": "wait-on dist/esigndoccontrol/public-api.d.ts && gulp build-assets --option esigndoccontrol",
    "dev": "npm run clean && npm-run-all --parallel serve-watch lib-watch build-assets",
    "build-lib": "npm run clean && npm-run-all --parallel lib-build build-assets",
    "lib-dispatch": "npm run build-lib && cd dist/esigndoccontrol && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "@types/fabric": "^3.6.7",
    "hammerjs": "^2.0.8",
    "ng2-pdf-viewer": "^6.3.2",
    "rxjs": "~6.4.0",
    "signature_pad": "^3.0.0-beta.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.10",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.10",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "graceful-fs":"^3.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-flatten": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.3",
    "gulp-uglifycss": "^1.0.6",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "update-electron-app": "^1.5.0",
    "wait-on": "^5.1.0"
  }
}
